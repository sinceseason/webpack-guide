{
  "name": "nodewebpackproj",
  "version": "1.0.0",
  "description": "node(koa2) + webpack + others",
  "main": "index.js",
  "scripts": {
    "start": "run-p server dev:node",
    "server": "cross-env NODE_ENV=development ENV=dev webpack-dev-server --inline --process --config webpack/build/webpack.dev.conf.js",
    "build:pro": "cross-env NODE_ENV=production ENV=dev webpack --config webpack/build/webpack.prod.conf.js",
    "build:dev": "cross-env NODE_ENV=development ENV=dev webpack --config webpack/build/webpack.dev.conf.js",
    "dev:node": "cross-env NODE_ENV=development ENV=dev nodemon ./node/bin/www.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "ignore": [
      "src",
      "package.json",
      "package-lock.json"
    ],
    "delay": "1000",
    "ext": "js,html"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fast-css-loader": "^1.0.2",
    "fast-sass-loader": "^1.4.6",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "log4js": "^3.0.5",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bluebird": "^3.5.2",
    "dotenv": "^6.1.0",
    "handlebars": "^4.0.12",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
    "nunjucks": "^3.1.3",
    "request": "^2.88.0"
  }
}
