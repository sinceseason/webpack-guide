{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p server dev",
    "start:webcat": "run-p server:webcat dev",
    "server": "cross-env NODE_ENV=development ENV=dev webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "webpack": "cross-env NODE_ENV=development ENV=dev webpack --config build/webpack.dev.conf.js",
    "server:webcat": "cross-env NODE_ENV=development ENV=dev WEBCAT_ENV=webcat webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev": "gulp build:env --env=dev&& cross-env NODE_ENV=development  ENV=dev nodemon --delay 1000ms -e js,html bin/www.js",
    "build:webpack:development": "cross-env NODE_ENV=development webpack --config build/webpack.dev.conf.js",
    "build:webpack:production": "cross-env NODE_ENV=production webpack --config build/webpack.pro.conf.js",
    "build:sit": "cross-env NODE_ENV=production ENV=sit webpack --config build/webpack.pro.conf.js && gulp build:production --env=sit",
    "build:pre": "cross-env NODE_ENV=production ENV=pre webpack --config build/webpack.pro.conf.js && gulp build:production --env=pre",
    "build:pro": "cross-env NODE_ENV=production ENV=pro webpack --config build/webpack.pro.conf.js && gulp build:production --env=pro",
    "prd": "pm2 start bin/www.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "qa": "jsinspect ./src/js",
    "qa2": "jscpd"
  },
  "nodemonConfig": {
    "ignore": [
      "public",
      "src",
      "package.json",
      "build",
      "config",
      "node_modules"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bluebird": "^3.5.1",
    "cross-env": "^5.1.3",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.4.0",
    "jquery": "^3.3.1",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.1.1",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
    "log4js": "^3.0.6",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "numeral": "^2.0.6",
    "nunjucks": "~3.1.3",
    "qr-image": "^3.2.0",
    "request": "^2.88.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fast-css-loader": "^1.0.2",
    "fast-sass-loader": "^1.4.6",
    "file-loader": "^2.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-zip": "^4.1.0",
    "happypack": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.8.1",
    "npm-run-all": "^4.1.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
}
